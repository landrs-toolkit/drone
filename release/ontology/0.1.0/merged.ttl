        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:equipmentType ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:modelNumber ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:registrationDate ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:registrationNumber ],
        [ a owl:Restriction ;
            owl:minCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:hasIdentifier ],
        [ a owl:Restriction ;
            owl:minCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:identifier ],
        [ a owl:Restriction ;
            owl:minCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:manufacturedBy ],
        [ a owl:Restriction ;
            owl:minCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:operatedBy ],
        owl:Thing ;
    skos:exactMatch <https://www.wikidata.org/wiki/Q10273457> ;
    prov:wasQuotedFrom "https://www.wikidata.org/wiki/Q10273457" .

drone: a owl:Ontology ;
    dcterms:abstract "The drone ontology was developed as part of the Slone Foundation fundedLinked-data API for Networked DRoneS using a Ontology Design Pattern (ODP) Based Approach " ;
    dcterms:contributor [ a sdo:Person ;
            sdo:affiliation "University of Notre Dame" ;
            sdo:name "Christopher Sweet" ;
            sdo:url "https://orcid.org/0000-0001-8214-7177"^^xsd:anyURI ],
        [ a sdo:Person ;
            sdo:affiliation "University of Notre Dame" ;
            sdo:name "Priscila Saboia Moreira" ] ;
    dcterms:created "2022-05-14"^^xsd:date ;
    dcterms:creator [ a sdo:Person ;
            sdo:affiliation "University of Notre Dame" ;
            sdo:email "cvardema@nd.edu"^^xsd:anyURI ;
            sdo:name "Charles F. Vardeman II" ;
            sdo:url "https://orcid.org/0000-0003-4091-6059"^^xsd:anyURI ] ;
    dcterms:description """The DRONE Ontology is intended to be a light weight Ontology for drone operations constructed from Ontology Design Patterns connecting and aligning OGC and W3C standard Ontologies.
This ontology is developed as part of the Sloan Funded LANDRS project to advance UxV data interoperability. The Merology was extracted from the Semantic Science Integrated Ontology to provide a lightweight set of parthood relations. 
Contributions and Discussion Welcomed.
Github: https://github.com/landrs-tookkit/drone"""@en ;
    dcterms:license <https://spdx.org/licenses/CC0-1.0> ;
    dcterms:modified "2022-07-05"^^xsd:date ;
    dcterms:publisher [ a sdo:Organization ;
            owl:sameAs wd:Q178848 ;
            sdo:name "Center for Research Computing, University of Notre Dame" ;
            sdo:url "https://crc.nd.edu"^^xsd:anyURI ] ;
    dcterms:rights "This vocabulary is distributed under Creative Commons Attribution Zero 1.0 Universal License - https://creativecommons.org/share-your-work/public-domain/cc0"@en ;
    dcterms:status "Draft" ;
    dcterms:title "LANDRS Ontology for Drone Operations"@en ;
    vann:preferredNamespacePrefix "drone" ;
    vann:preferredNamespaceUri "http://w3id.org/drone/ont/" ;
    rdfs:seeAlso <https://www.landrs.org> ;
    owl:imports <https://raw.githubusercontent.com/opengeospatial/ogc-geosparql/master/1.1/geo.ttl>,
        <https://raw.githubusercontent.com/w3c/sdw/gh-pages/time-aggregates/rdf/time-agg.ttl>,
        <https://raw.githubusercontent.com/w3c/sdw/gh-pages/time/rdf/time.ttl>,
        <https://www.w3.org/ns/prov-o> ;
    owl:versionIRI drone:0.1.0 ;
    owl:versionInfo "0.1.0"@en ;
    skos:note "Prior development of the Drone ontology was in the Github https://github.com/landrs-toolkit/LANDRS-o. Drone refactored and merged LANDRS-o and is now depricated." ;
    prov:wasInfluencedBy <https://data.nasa.gov/ontologies/atmonto/ATM#>,
        <https://data.nasa.gov/ontologies/atmonto/equipment#>,
        <https://docs.ogc.org/per/20-020.html#Ontologies-Component>,
        <https://semantics.aero/pages/swim-vocabulary#> ;
    sdo:license "https://spdx.org/licenses/CC0-1.0"@en .

