    rdfs:domain sio:Entity ;
    rdfs:isDefinedBy drone: ;
    rdfs:range sio:Entity ;
    skos:exactMatch sio:SIO_000001 ;
    sdo:comment "'is related to' is the top level relation in SIO" .

drone:lastSegment a owl:ObjectProperty ;
