            owl:allValuesFrom xsd:string ;
            owl:onProperty drone:identifierAsText ] .

drone:Aircraft a owl:Class ;
    rdfs:label "Aircraft"@en ;
    rdfs:comment "Machine that is able to fly by gaining support from the air other than the reactions of the air against the earth’s surface"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom drone:AircraftManufacturer ;
            owl:onProperty drone:manufacturedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom drone:AircraftOperator ;
            owl:onProperty drone:operatedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom drone:AircraftOwner ;
            owl:onProperty drone:ownedBy ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:callSign ],
        drone:Vehicle ;
    skos:exactMatch wd:Q11436 ;
    prov:wasQuotedFrom "https://www.wikidata.org/wiki/Q11436" .

drone:Event a owl:Class ;
    rdfs:label "Event" ;
    rdfs:subClassOf time:TemporalEntity ;
    owl:equivalentClass prov:InstantaneousEvent ;
    skos:definition "An instantaneous event, or event for short, happens in the world and marks a change in the world, in its activities and in its entities."@en ;
    prov:wasQuotedFrom "https://www.w3.org/TR/prov-o/#InstantaneousEvent"@en .

drone:identifierAsText a owl:DatatypeProperty ;
    rdfs:label "identifierAsText" ;
    oplax:isNativeTo "https://archive.org/services/purl/domain/modular_ontology_design_library/identifier"^^rdfs:Literal .

drone:operatedBy a owl:ObjectProperty ;
    rdfs:label "operator" ;
    rdfs:range prov:Agent .

drone:segment a owl:ObjectProperty ;
