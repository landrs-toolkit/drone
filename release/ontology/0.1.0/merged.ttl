<http://purl.org/goodrelations/v1#BusinessEntity> a rdfs:Class,
        owl:Class ;
    rdfs:label "Business entity" ;
    rdfs:subClassOf org:FormalOrganization .

drone:ASV a rdfs:Class ;
    rdfs:label "Autonomous Surface Vehicle"@en ;
    rdfs:comment "Vehicle that operates on the surface of the water without a crew"@en ;
    rdfs:subClassOf drone:AutonomousRobot,
        drone:SurfaceRobot ;
    skos:altLabel "ASV" ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:AUV a rdfs:Class ;
    rdfs:label "Autonomous Underwater Vehicle"@en ;
    rdfs:comment "An autonomous underwater vehicle (AUV) is a robot that travels underwater without requiring input from an operator."@en ;
    rdfs:subClassOf drone:AutonomousRobot,
        drone:UnderwaterRobot ;
    skos:altLabel "AUV"@en ;
    skos:exactMatch wd:Q257406 ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:AerialRobot a rdfs:Class ;
    rdfs:label "Aerial Robot"@en ;
    rdfs:comment "Aerial Robots are vehicles that move autonomously in the air."@en ;
    rdfs:subClassOf drone:Aircraft,
        drone:RoboticVehice .

drone:Airframe a rdfs:Class ;
    rdfs:label "Airframe"@en ;
    rdfs:comment "The mechanical structure of an aircraft is known as the airframe. This structure is typically considered to include the fuselage, undercarriage, empennage and wings, and exclude the propulsion system."@en ;
    rdfs:subClassOf drone:Body ;
    skos:exactMatch wd:Q222946 .

drone:AutomatedRobot a rdfs:Class ;
    rdfs:label "Automated Robot"@en ;
    rdfs:subClassOf drone:Robot ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:AutonomousRobot a rdfs:Class ;
    rdfs:label "Autonomous Robot"@en ;
    rdfs:comment "An autonomous robot is a robot that performs behaviors or tasks with a high degree of autonomy (without external influence)."@en ;
    rdfs:subClassOf wd:Q2916098,
        drone:Robot ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:Autopilot a rdfs:Class ;
    rdfs:label "Autopilot"@en ;
    rdfs:comment "Software to control and navigate an aircraft"@en ;
    rdfs:subClassOf sdo:SoftwareApplicaion ;
    skos:altLabel "Flight Controller"@en,
        "Flight Controller Board"@en ;
    sdo:sameAs s3n:Algorithm ;
    drone:component drone:FlightControlSystem ;
    drone:isPartOf [ a drone:Component ;
            drone:hasWhole drone:FlightControlSystem ] .

drone:Body a rdfs:Class,
        drone:UAVComponent ;
    rdfs:label "UxV Body"@en ;
    rdfs:comment "Body of the UxV, for a UAV this would be an airframe"@en ;
    rdfs:subClassOf drone:Product ;
    drone:component drone:UxV ;
    drone:isPartOf [ a drone:Component,
                drone:UAVComponent ;
            drone:hasWhole drone:UxV ] .

drone:Buoy a rdfs:Class ;
    rdfs:label "Buoy"@en ;
    rdfs:comment "A floating device that can be anchored (stationary) or allowed to drift with ocean currents."@en ;
    rdfs:subClassOf drone:UnmotorizedVehicle ;
    skos:exactMatch wd:Q30026 ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:CommunicationSystem a rdfs:Class ;
    rdfs:label "Communication System"@en ;
    rdfs:comment "Communication System, frequency and protocol by which a UxS communicates. For example Civilian UAV communications systems typically operate on frequencies of 2.4 GHz and 5.8 GHz. UAV communications systems work by using one frequency to control the aerial vehicle from the ground via a remote pilot while the other frequency is used to beam or relay First-Person View (FPV) video. By utilizing high-quality, reliable communications links, civilian UAVs are able to relay aerial visuals and data to those on the ground with ease, while still remaining in flight."@en ;
    rdfs:subClassOf ssn:System,
        drone:Product ;
    skos:exactMatch wd:Q31479975 ;
    sdo:sameAs s3n:CommunicatingSystem ;
    drone:component drone:UAS ;
    drone:isPartOf [ a drone:Component,
                drone:UAVComponent ;
            drone:hasWhole drone:UAS ] .

drone:CompanionComputer a rdfs:Class ;
    rdfs:label "Companion Computer"@en ;
    rdfs:comment "A companion computer is a device that travels on-board the vehicle and controls/communicates with the autopilot over a low-latency link. Apps running on a companion computer can perform computationally intensive or time-sensitive tasks, and add much greater intelligence than is provided by the autopilot alone." ;
    rdfs:seeAlso s3n:MicroController ;
    rdfs:subClassOf ssn:System,
        drone:Payload,
        drone:Product ;
    skos:exactMatch wd:Q944780 .

drone:Component a rdfs:Class ;
    rdfs:label "Component"@en ;
    rdfs:comment "Subclass of equipment to enumerate composition of a UxV"@en ;
    rdfs:subClassOf drone:Equipment ;
    skos:exactMatch wd:Q811909 .

drone:EquipmentOperator a rdfs:Class,
        owl:Class ;
    rdfs:label "Equipment operator" ;
    rdfs:subClassOf prov:Agent ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty drone:operatorOf ;
            owl:someValuesFrom drone:Equipment ] ;
    skos:definition "The Agent who is operating drone equipment"@en .

drone:FlightControlSystem a rdfs:Class ;
    rdfs:label "Flight Control System"@en ;
    rdfs:comment "Aircraft system utilized to control flight surfaces"@en ;
    skos:exactMatch wd:Q11481471 .

drone:FlightControllerBoard a rdfs:Class ;
    rdfs:label "Flight Controller Board"@en ;
    rdfs:comment "A flight controller (FC) is a small circuit board of varying complexity. Its function is to direct the RPM of each motor in response to input. A command from the pilot for the multi-rotor to move forward is fed into the flight controller, which determines how to manipulate the motors accordingly."@en ;
    rdfs:seeAlso <https://dronekit-python.readthedocs.io/en/latest/develop/companion-computers.html> ;
    rdfs:subClassOf sosa:System,
        drone:Product ;
    sdo:sameAs s3n:MicroController ;
    drone:component drone:FlightControlSystem ;
    drone:isPartOf [ a drone:Component ;
            drone:hasWhole drone:FlightControlSystem ] .

drone:GroundControlStation a rdfs:Class ;
    rdfs:label "Ground Control Station"@en ;
    rdfs:comment "A ground control station (GCS) is a land- or sea-based control center that provides the facilities for human control of Unmanned Aerial Vehicles (UAVs or drones)."@en ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Ground_control_station> ;
    rdfs:subClassOf drone:Product ;
    skos:exactMatch wd:Q4118990 ;
    drone:component drone:UAS ;
    drone:isPartOf [ a drone:Component ;
            drone:hasWhole drone:UAS ] .

drone:MotorizedVehicle a rdfs:Class ;
    rdfs:label "Motorized Vehicle"@en ;
    rdfs:subClassOf drone:Vehicle ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:Payload a rdfs:Class,
        owl:Class ;
    rdfs:label "Payload"@en ;
    rdfs:comment "Mission load on a vehicle"@en,
        "Unmanned vehicle payload"@en ;
    rdfs:subClassOf wd:Q319224,
        drone:Cargo ;
    skos:closeMatch <https://www.wikidata.org/wiki/Q21211206> ;
    drone:component drone:UAS ;
    drone:isPartOf [ a drone:Component ;
            drone:hasWhole drone:UAS ] .

drone:ROV a rdfs:Class ;
    rdfs:label "Remotely Operated Underwater Vehicle"@en ;
    rdfs:subClassOf drone:MotorizedVehicle,
        drone:UnderwaterRobot ;
    skos:altLabel "ROV"@en ;
    skos:exactMatch wd:Q618465 ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:RemotelyPilotedRobot a rdfs:Class ;
    rdfs:label "Remotely Piloted Robot"@en ;
    rdfs:subClassOf drone:Robot ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:Robot a rdfs:Class ;
    rdfs:label "Robot"@en ;
    rdfs:comment "Mechanical or virtual artificial agent carrying out physical activities"@en ;
    rdfs:subClassOf drone:Product ;
    skos:exactMatch wd:Q11012 ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:RoboticVehicle a rdfs:Class ;
    rdfs:label "Robotic Vehicle"@en ;
    rdfs:comment "Robotic vehicles are machines that move autonomously on the ground, in the air, undersea, or in space. Such vehicles are “unmanned,” in the sense that no humans are on board. In general, these vehicles move by themselves, under their own power, with sensors and computational resources onboard to guide their motion."@en ;
    rdfs:subClassOf wd:Q4810574,
        drone:Robot,
        drone:Vehicle ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:SmartSensor a rdfs:Class,
        owl:Class ;
    rdfs:label "Smart Sensor"@en ;
    rdfs:comment "Smart-Sensor - A SmartSensor is composed of one or more Sensors together with a MicroController that implements different Algorithms, and makes Algorithm Executions on the result of the Observations these Sensors make to output a resulting value for some Indicator. This value may then be communicated by some CommunicatingSystemCommunicatingSystem."@en ;
    rdfs:subClassOf sosa:Platform,
        ssn:System ;
    sdo:sameAs s3n:SmartSensor .

drone:SurfaceRobot a rdfs:Class ;
    rdfs:label "Underwater Robot"@en ;
    rdfs:comment "Surface Robots are vehicles that move autonomously on the water surface."@en ;
    rdfs:subClassOf drone:RoboticVehicle,
        drone:SurfaceVehicle ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:SurfaceVehicle a rdfs:Class ;
    rdfs:label "Surface Vehicle"@en ;
    rdfs:subClassOf drone:Vehicle ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:UAS a rdfs:Class ;
    rdfs:label "Unmanned Aircraft System"@en ;
    rdfs:comment "An Unmanned Aerial Vehicle, ground-based controller and communication system."@en ;
    rdfs:subClassOf drone:Product ;
    skos:altLabel "UAS"@en ;
    skos:prefLabel "Unmanned Aircraft System"@en .

drone:UAV a rdfs:Class,
        owl:Class ;
    rdfs:label "Unmanned Aerial Vehicle"@en ;
    rdfs:comment "An aircraft without a human pilot on board and a type of unmanned vehicle. UAVs are a component of an unmanned aircraft system (UAS); which include a UAV, a ground-based controller, and a system of communications between the two. The flight of UAVs may operate with various degrees of autonomy: either under remote control by a human operator or autonomously by onboard computers." ;
    rdfs:subClassOf sosa:FeatureOfInterest,
        sosa:Platform,
        drone:AerialRobot,
        drone:Aircraft,
        drone:MotorizedVehicle,
        drone:UxV ;
    skos:altLabel "UAV"@en ;
    skos:exactMatch wd:Q484000 ;
    skos:prefLabel "Unmanned Aerial Vehicle"@en ;
    drone:component drone:UAS .

drone:UAVComponent a rdfs:Class ;
    rdfs:label "Unmanned Aircraft Component"@en ;
    rdfs:comment "Component of a Unmanned Aerial Vehicle"@en ;
    rdfs:subClassOf drone:Component ;
    skos:exactMatch wd:Q28816538 .

drone:USV a rdfs:Class ;
    rdfs:label "Unmanned Surface Vehicle"@en ;
    rdfs:comment "Vehicle that operates on the surface of the water without a crew"@en ;
    rdfs:subClassOf drone:MotorizedVehicle,
        drone:SurfaceRobot ;
    skos:altLabel "USV"@en ;
    skos:exactMatch wd:Q4419860 ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:UnderwaterGlider a rdfs:Class ;
    rdfs:label "Underwater Glider"@en ;
    rdfs:comment "An underwater glider is a type of autonomous underwater vehicle (AUV) that employs variable-buoyancy propulsion instead of traditional propellers or thrusters. It employs variable buoyancy in a similar way to a profiling float, but unlike a float, which can only move up and down, an underwater glider is fitted with hydrofoils (underwater wings) that allow it to glide forward while descending through the water. At a certain depth, the glider switches to positive buoyancy to climb back up, and the cycle is then repeated."@en ;
    rdfs:subClassOf drone:AUV,
        drone:UnmotorizedVehicle ;
    skos:exactMatch wd:Q2634473 ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:UnderwaterRobot a rdfs:Class ;
    rdfs:label "Underwater Robot"@en ;
    rdfs:comment "Underwater Robots are vehicles that move autonomously underwater."@en ;
    rdfs:subClassOf drone:RoboticVehice,
        drone:UnderwaterVehicle ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:UnderwaterVehicle a rdfs:Class ;
    rdfs:label "Underwater Vehicle"@en ;
    rdfs:subClassOf drone:Vehicle ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:UnmotorizedVehicle a rdfs:Class ;
    rdfs:label "Unmotorized Vehicle"@en ;
    rdfs:subClassOf drone:Vehicle ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:UxV a rdfs:Class ;
    rdfs:label "Unmanned Vehicle"@en ;
    rdfs:comment "An unmanned vehicle or uncrewed vehicle is a vehicle without a person on board. Uncrewed vehicles can either be remote controlled or remote guided vehicles, or they can be autonomous vehicles which are capable of sensing their environment and navigating on their own."@en ;
    rdfs:subClassOf sosa:Platform,
        drone:Agent,
        drone:Vehicle ;
    skos:altLabel "UxV"@en ;
    skos:exactMatch wd:Q1527901 ;
    skos:prefLabel "Unmanned Vehicle"@en .

drone:Vehicle a rdfs:Class ;
    rdfs:label "Vehicle"@en ;
    rdfs:comment "A vehicle is a device that is designed or used to transport people or cargo over land, water, air, or through space."@en ;
    rdfs:subClassOf drone:Equipment ;
    skos:exactMatch wd:Q42889 ;
    sdo:sameAs sdo:Vehicle .

drone:Vessel a rdfs:Class ;
    rdfs:label "Vessel"@en ;
    rdfs:comment "Vehicle that is a watercraft, boat, or ship. Note: Wikidata definition of vessel includes spaceship this drone:Vessel is a subClassOf wikidata vessel."@en ;
    rdfs:subClassOf drone:MotorizedVehicle,
        drone:SurfaceVehicle ;
    skos:exactMatch wd:Q16391167 ;
    prov:wasDerivedFrom [ a prov:Entity ;
            dcterms:source <https://www.mdpi.com/185614> ;
            dcterms:title "SWARMs Ontology: A Common Information Model for the Cooperation of Underwater Robots"@en ;
            sdo:identifier "https://dx.doi.org/10.3390%2Fs17030569" ] .

drone:AircraftModel a owl:Class ;
    rdfs:label "Aircraft model" ;
    rdfs:subClassOf drone:EquipmentModel .

drone:AircraftType a owl:Class ;
    rdfs:label "Aircraft type" ;
    rdfs:subClassOf drone:EquipmentType .

drone:Arrival a owl:Class ;
    rdfs:label "Arrival" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:airport ],
        time:Instant,
        prov:End,
        drone:Event .

drone:Departure a owl:Class ;
    rdfs:label "Departure" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:airport ],
        time:Instant,
        prov:Start,
        drone:Event .

drone:Entity a owl:Class ;
    rdfs:label "Entity"@en ;
    dcterms:description "Every thing is an entity."@en ;
    rdfs:isDefinedBy drone: ;
    owl:equivalentClass prov:Entity ;
    skos:exactMatch sio:SIO_000000 .

drone:Flight a owl:Class ;
    rdfs:label "Flight" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom drone:AircraftOperator ;
            owl:onProperty drone:operatedBy ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:arrival ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:departure ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:flightCategory ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:flightNumber ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:actualRoute ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:plannedRoute ],
        [ a owl:Restriction ;
            owl:minCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onProperty drone:aircraft ],
        drone:Mission .

drone:InstrumentPayload a owl:Class ;
    rdfs:label "Instrument Payload"@en ;
    rdfs:comment "Payload that is a instrument platform such as cameras, atmospheric sensors, etc"@en ;
    rdfs:subClassOf ssn:System,
        drone:Payload ;
    skos:exactMatch <https://www.wikidata.org/wiki/Q3099911> .

drone:filedRoute a owl:ObjectProperty ;
    rdfs:label "filed route" ;
    rdfs:subPropertyOf drone:hasRoute .

drone:hasComponentPart a owl:ObjectProperty ;
    rdfs:label "has component part"@en ;
    dcterms:description "has component part is a relation between a whole and a component part where the component is instrinsic to the whole, and loss of the part would change the kind that it is."@en ;
    rdfs:isDefinedBy drone: ;
    rdfs:subPropertyOf drone:hasDirectPart ;
    owl:inverseOf drone:isComponentPartOf ;
    skos:exactMatch sio:SIO_000369 ;
    skos:example "a human has blood as a component part"@en .

drone:hasSubactivity a owl:ObjectProperty ;
    rdfs:label "hasSubactivity" ;
    rdfs:domain drone:Activity ;
    rdfs:range drone:Activity ;
    rdfs:subPropertyOf drone:hasPart .

drone:isCarriedBy a owl:ObjectProperty ;
    rdfs:label "is carried by"@en ;
    rdfs:domain drone:Vehicle ;
    rdfs:range drone:Cargo ;
    rdfs:subPropertyOf drone:isPartOf ;
    owl:inverseOf drone:carries .

drone:isSurroundedBy a owl:ObjectProperty ;
    rdfs:label "is surrounded by"@en ;
    dcterms:description "A 'is surrounded by' B iff the A 'is contained by' B and A 'is adjacent to' B or A 'is directly connected to' B."@en ;
    rdfs:isDefinedBy drone: ;
    rdfs:subPropertyOf drone:isContainedIn ;
    owl:inverseOf drone:surrounds ;
    skos:exactMatch sio:SIO_000323 .

drone:manufactures a owl:ObjectProperty ;
    rdfs:label "manufactures" ;
    rdfs:range drone:Equipment ;
    owl:inverseOf drone:manufacturedBy .

<https://w3id.org/drone/ont/module/activity> a oplax:Module ;
    rdfs:comment "Drone core module for Events and Activities" ;
    rdfs:seeAlso <https://docs.ogc.org/per/20-020.html#_flight_ontology> ;
    skos:note "Was extracted from the OGC Testbed 16: Aviation Engineering Report" .

<https://w3id.org/drone/ont/module/droneeqp> a oplax:Module ;
    rdfs:comment "Uses Trajectory Ontology Design Pattern"@en ;
    rdfs:seeAlso <https://docs.ogc.org/per/20-020.html#_equipment_ontology>,
        <https://docs.ogc.org/per/20-020.html#_route_ontology> ;
    oplax:reusesAsTemplate <https://archive.org/services/purl/domain/modular_ontology_design_library/trajectory> .

