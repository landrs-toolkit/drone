<https://w3id.org/drone/ont/module/vehicle> rdf:type oplax:Module ;
  rdfs:seeAlso <https://docs.ogc.org/per/20-020.html#_aircraft_ontology> ;
  skos:note "This is a generalization of the OGC Aviation ontology to support other vehicle classes than Aircraft. "
.
drone:Vehicle a rdfs:Class ;
  rdfs:label "Vehicle"@en ;
  rdfs:comment "A vehicle is a device that is designed or used to transport people or cargo over land, water, air, or through space."@en ;
  rdfs:subClassOf drone:Equipment ;
  sdo:sameAs sdo:Vehicle ;
  skos:exactMatch wd:Q42889 ;
.

drone:Aircraft
  a owl:Class ;
  rdfs:label "Aircraft"@en ;
  rdfs:comment "Machine that is able to fly by gaining support from the air other than the reactions of the air against the earth’s surface"@en ;
  prov:wasQuotedFrom "https://www.wikidata.org/wiki/Q11436";
  rdfs:subClassOf drone:Vehicle ;
  skos:exactMatch wd:Q11436 ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom drone:AircraftManufacturer ;
      owl:onProperty drone:manufacturedBy ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom drone:AircraftOperator ;
      owl:onProperty drone:operatedBy ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom drone:AircraftOwner ;
      owl:onProperty drone:ownedBy ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty drone:callSign ;
    ] ;
.
drone:AircraftManufacturer
  a owl:Class ;
  rdfs:label "Aircraft manufacturer" ;
  skos:exactMatch wd:Q107009743;
  rdfs:subClassOf drone:Manufacturer ;
.
drone:AircraftModel
  a owl:Class ;
  rdfs:label "Aircraft model" ;
  rdfs:subClassOf drone:EquipmentModel ;
.
drone:AircraftOperator
  a owl:Class ;
  rdfs:label "Aircraft operator" ;
  rdfs:subClassOf drone:EquipmentOperator ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty drone:operatorOf ;
      owl:someValuesFrom drone:Equipment ;
    ] ;
.
drone:AircraftOwner
  a owl:Class ;
  rdfs:label "Aircraft owner" ;
  rdfs:subClassOf drone:EquipmentOwner ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty drone:ownerOf ;
      owl:someValuesFrom drone:Aircraft ;
    ] ;
.
drone:AircraftType
  a owl:Class ;
  rdfs:label "Aircraft type" ;
  rdfs:subClassOf drone:EquipmentType ;
.
drone:aircraft
  a owl:ObjectProperty ;
  rdfs:label "aircraft" ;
  rdfs:range drone:Aircraft ;
.
drone:callSign
  a owl:DatatypeProperty ;
  rdfs:label "call sign" ;
  rdfs:subPropertyOf drone:identifierAsText;
.
###  http://schema.drone.org/schema/UAV
drone:UAV a rdfs:Class, owl:Class ;
  rdfs:label "Unmanned Aerial Vehicle"@en ;
  rdfs:comment "An aircraft without a human pilot on board and a type of unmanned vehicle. UAVs are a component of an unmanned aircraft system (UAS); which include a UAV, a ground-based controller, and a system of communications between the two. The flight of UAVs may operate with various degrees of autonomy: either under remote control by a human operator or autonomously by onboard computers." ;
  skos:prefLabel "Unmanned Aerial Vehicle"@en ;
  skos:altLabel "UAV"@en ;
  skos:exactMatch wd:Q484000 ;
  rdfs:subClassOf drone:UxV ,
                  drone:Aircraft ,
                  drone:AerialRobot ,
                  drone:MotorizedVehicle ,
                  sosa:FeatureOfInterest ,
                  sosa:Platform ;
  drone:component drone:UAS .


